{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=ironambit.db",
    "Provider": "Sqlite"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
        "Microsoft.EntityFrameworkCore.Database.Transaction": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Connection": "Information",
        "Microsoft.EntityFrameworkCore.Infrastructure": "Debug",
        "Microsoft.EntityFrameworkCore.Query": "Debug",
        "Microsoft.EntityFrameworkCore.Update": "Debug",
        "Microsoft.EntityFrameworkCore.Migrations": "Information",
        "System": "Warning",
        "Application.Behaviors": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithEnvironmentName"
    ],
    "Properties": {
      "Application": "IronAmbit Service"
    }
  }
}